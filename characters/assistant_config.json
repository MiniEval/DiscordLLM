    "stop": ["\n\n", "#", "***", "@", "</s>", "<|im_end|>"],
    "early_stopping": false,
    "add_bos_token": true,
    "ban_eos_token": false,
    "skip_special_tokens": true
  },
  "summary_params": {
    "max_tokens": 1024,
    "truncation_length": 8192,
    "do_sample": true,
    "temperature": 0.25,
    "temperature_last": false,
    "min_p": 0.05,
    "top_p": 1.0,
    "top_k": 0,
    "repetition_penalty": 1.05,
    "stop": ["\n\n", "#", "***", "@", "</s>", "<|im_end|>"],
    "early_stopping": false,
    "add_bos_token": true,
    "ban_eos_token": false,
    "skip_special_tokens": true
  },
  "bot_token": "[INSERT BOT TOKEN HERE]",
  "admins": [] (Insert Discord user IDs here),
  "channel_id": 0 (Insert Discord channel ID here)
}