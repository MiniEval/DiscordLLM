{
  "context": "./characters/assistant_context.json",
  "persona": "./characters/assistant_persona.json",
  "chat_format": "./formats/alpaca_chat.json",
  "summary_format": "./formats/alpaca_summary.json",
  "banned_substrings": [".com", ".net", "http", ".gg", ".org", ".gov", ".edu"],
  "max_tokens": 8192,
  "chat_params": {
    "max_tokens": 120,
    "truncation_length": 8192,
